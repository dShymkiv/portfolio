{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "ts-node app.ts",
    "lint:fix": "npm run eslint . --fix",
    "eslint": "eslint .",
    "start docker containers": "docker-compose -f docker-compose.db.yaml up -d",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm migration:revert --dataSource=./db/dataSource.js",
    "migration:generate": "npm run typeorm migration:create -n $npm_config_name --dataSource=./db/dataSource.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^14.0.0",
    "pg": "^8.12.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.2.45",
    "typescript": "^5.5.2",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/joi": "^14.3.3",
    "@types/pg": "^8.11.6",
    "@types/eslint": "^8.21.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.8.3",
    "eslint-plugin-import": "^2.25.4"
  }
}
